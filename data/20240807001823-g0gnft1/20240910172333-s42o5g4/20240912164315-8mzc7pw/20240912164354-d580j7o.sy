			"Children": [
				{
					"Type": "NodeCodeBlockFenceOpenMarker",
					"Data": "```"
				},
				{
					"Type": "NodeCodeBlockFenceInfoMarker",
					"CodeBlockInfo": "bWF0bGFi"
				},
				{
					"Type": "NodeCodeBlockCode",
					"Data": "% 生成样本数据\nx1 = (1:10)';  % 自变量 x1\nx2 = (10:-1:1)';  % 自变量 x2\ny = [2.5, 3.5, 6.7, 10.8, 20.1, 36.5, 68.9, 131.7, 253.5, 490.4]';  % 因变量\n\n% 将自变量组合成矩阵\nX = [x1, x2];\n\n% 定义多元非线性模型：y = β1 * exp(β2 * x1 + β3 * x2)\nmodelfun = @(beta, X) beta(1) * exp(beta(2) * X(:, 1) + beta(3) * X(:, 2));\n\n% 初始参数估计\nbeta0 = [1, 0.1, 0.1];\n\n% 进行多元非线性回归拟合\nbeta = nlinfit(X, y, modelfun, beta0);\n\n% 预测值计算\nx1_pred = linspace(min(x1), max(x1), 100)';\nx2_pred = linspace(min(x2), max(x2), 100)';\nX_pred = [x1_pred, x2_pred];\ny_pred = modelfun(beta, X_pred);\n\n% 输出拟合的参数\ndisp('拟合的参数:');\ndisp(beta);\n\n% 绘制拟合曲线（使用 x1 与 y_pred）\nfigure;\nplot3(x1, x2, y, 'bo', 'MarkerFaceColor', 'b');  % 原始数据点\nhold on;\nplot3(x1_pred, x2_pred, y_pred, 'r-', 'LineWidth', 2);  % 拟合曲线\nxlabel('X1');\nylabel('X2');\nzlabel('Y');\ntitle('多元非线性回归 (指数模型)');\nlegend('原始数据', '拟合曲线');\ngrid on;\n\n"
				},
				{
					"Type": "NodeCodeBlockFenceCloseMarker",
					"Data": "```"
				}
			]
		},
